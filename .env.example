# PostgreSQL Configuration
# Copy this file to .env and modify the values as needed

# Database credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=myapp

# Connection settings
POSTGRES_PORT=5432

# Timezone (optional, defaults to UTC)
#TZ=UTC
TZ=America/Santiago

# Example values:
# TZ=America/New_York
# TZ=Europe/Madrid
# TZ=Asia/Tokyo
